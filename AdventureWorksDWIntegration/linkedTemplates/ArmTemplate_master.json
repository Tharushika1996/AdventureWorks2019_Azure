{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AdventureWorksDWIntegration"
		},
		"AdventureWorksDW_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "newserverone.database.windows.net"
		},
		"AdventureWorksDW_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksDW"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AdventureWorksDWIntegration_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AdventureWorksDW_properties_typeProperties_server": {
						"value": "[parameters('AdventureWorksDW_properties_typeProperties_server')]"
					},
					"AdventureWorksDW_properties_typeProperties_database": {
						"value": "[parameters('AdventureWorksDW_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}